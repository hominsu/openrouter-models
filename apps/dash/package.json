{
  "name": "openrouter-models-dash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "yarn next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev",
    "deploy": "yarn pages:build && wrangler pages deploy",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/openrouter-models-dash.eslintcache",
    "format:check": "prettier --check --ignore-path .prettierignore .",
    "format:write": "prettier --write --ignore-path .prettierignore .",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "@openrouter-models/db": "workspace:^",
